# Supabase設定
# NEXT_PUBLIC_SUPABASE_URLまたはSUPABASE_URLのどちらかを設定
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# サーバー設定
PORT=3001
NODE_ENV=development

# ログレベル
LOG_LEVEL=debug

# メインシステムURL
# NEXT_PUBLIC_BASE_URLまたはMAIN_SYSTEM_URLのどちらかを設定
NEXT_PUBLIC_BASE_URL=https://line-auto-reply.vercel.app

# セキュリティ
API_KEY=your_secure_api_key_here
SYSTEM_TOKEN=your_system_integration_token_here

# 署名検証（開発環境ではスキップ可能）
SKIP_SIGNATURE_VERIFICATION=false